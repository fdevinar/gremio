<!DOCTYPE html>
<head>
    <title>Movie Database</title>
    <link rel="stylesheet" type="text/css" href="/app.css">
</head>
<body>
    <div>
        <h1>MOVIE DETAILS</h1>
       
        <% console.log(movieDetail); %>

        <div id="movieDetailBlock">
            <ul>
                <% for (i=0; i<movieDetail.length; i++){ %>
                    <% console.log(i); %>
                    <%= i %>
                <% }; %>


                <% Object.entries(movieDetail).forEach(entry => { %>
                <li>
                <%    let key = entry[0]; %>
                <%    let value = entry[1]; %>
                </li>
                <strong><%= key %>:</strong>
                <%= value %>
                <%  }); %>


            </ul>
        </div>

        

    </div>
</body>
</html>

