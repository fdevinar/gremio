<%- include('partials/header') %>

<h1>Friends List</h1>

<ul>
<% friendList.forEach(function(friend){ %>
    <li>
        <%= friend %>
    </li>
<% }); %>
</ul>

<p>Add a New Friend Below:</p>
<form action='/addfriend' method='POST'>
<input type='text' name='friendName' placeholder='name'>
<input type='submit'>
</form>

